{
  "name": "traffic-simulator-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.6",
    "google-map-react": "^1.0.4",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch-css": "npm run build-css && node-sass-chokidar --watch --recursive --skip-initial ./src/assets/styles/scss -o ./src/assets/styles/css --include-path ./node_modules --include-path ./src",
    "build-css": "node-sass-chokidar ./src/assets/styles/scss -o ./src/assets/styles/css --include-path ./node_modules --include-path ./src"
  }
}
